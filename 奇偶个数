import java.util.Scanner;

public class Main {
	
		public static void main(String[] args) 
		{
			Scanner in = new Scanner(System.in);
			int a = in.nextInt();
			int odd = 0;
			int even = 0;
			
			
			while(a!=-1)
			{
				if(a%2==0)
				{
					++even;
				}else
				{
					++odd;
				}
				
				a = in.nextInt();
			}
				System.out.println(odd+" "+even);
			


			
			
		}
	}
